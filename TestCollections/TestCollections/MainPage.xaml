<?xml
    version = "1.0"
    encoding = "utf-8" ?>
<ContentPage
    xmlns = "http://xamarin.com/schemas/2014/forms"
    xmlns:x = "http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:testCollections = "clr-namespace:TestCollections;assembly=TestCollections"
    xmlns:xaml = "clr-namespace:Xamarin.Forms.Xaml;assembly=Xamarin.Forms.Core"
    x:Class = "TestCollections.MainPage" >
    <ContentPage.BindingContext>
        <testCollections:MainPageViewModel/>
    </ContentPage.BindingContext>
    <ContentPage.Content>
        <CollectionView ItemsSource="{Binding ValueProvider.Values}"></CollectionView>
    </ContentPage.Content>
</ContentPage>

